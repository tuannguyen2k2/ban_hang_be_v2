{
    "version": 2,
    "builds": [
        {
            "src": "src/main.ts",
            "use": "now-node",
            "env": {
                "NODE_OPTIONS": "--ignore-errors=MODULE_NOT_FOUND"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "src/main.ts"
        }
    ]
}